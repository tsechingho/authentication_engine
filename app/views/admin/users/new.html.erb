<%# render :partial => 'admin/shared/restful_links', :object => @user %>
<% title t('admin.users.titles.new') %>

<%= error_messages_for :user %>

<% form_for [:admin, @user] do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit t('admin.users.actions.new') %>
  </p>
<% end %>
